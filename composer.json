  "type": "composer-plugin",
  "keywords": ["docker", "php"],
  "homepage": "https://github.com/utnaf/golem",
  "license": "WTFPL",
  "authors": [
    {
      "name": "Davide Effe",
      "email": "utnaf.dev@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
